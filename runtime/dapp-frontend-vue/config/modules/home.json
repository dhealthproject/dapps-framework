{
  "identifier": "home",
  "dependencies": {
    "waitFor": ["app/init"]
  },
  "namespaced": false,
  "routerConfig": {
    "/": {
      "identifier": "home",
      "formatters": {},
      "layout": "default",
      "cards": [
        {
          "identifier": "title-message",
          "component": "HelloWorld",
          "props": {
            "msg": "Welcome to your dApp with dHealth Network"
          }
        },
        {
          "identifier": "hi-button",
          "component": "ActionButton",
          "props": {
            "variant": "tertiary"
          }
        }
      ]
    },
    "/settings": {
      "identifier": "settings",
      "formatters": {},
      "layout": "default",
      "cards": [
        {
          "identifier": "title-message",
          "component": "TableCard",
          "state": {
            "getter": "app/getSettings"
          }
        }
      ]
    }
  }
}
