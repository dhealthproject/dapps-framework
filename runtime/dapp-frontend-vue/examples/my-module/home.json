{
  "identifier": "home",
  "formatters": {},
  "layout": "default",
  "cards": [
    {
      "identifier": "title-message",
      "component": "Card",
      "display": {
        "size": "full-width",
        "onEmpty": true,
        "onError": true,
        "classes": [
          "text-green-800",
          "text-bold"
        ]
      },
      "state": {
        "getter": "app/getInfo",
        "formatters": {
          "address": "format-mini-address",
          "balance": "format-absolute-amount"
        }
      }
    }
  ]
}
